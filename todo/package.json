{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "todo api made using nodejs",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "nodemon src/index.js",
    "make:migration": "yarn knex migrate:make --knexfile src/knexfile.js",
    "make:seeder": "yarn knex seed:make --knexfile src/knexfile.js",
    "seed": "yarn knex seed:run --knexfile src/knexfile.js",
    "migrate": "yarn knex migrate:latest --knexfile src/knexfile.js"
  },
  "keywords": [
    "todo",
    "api",
    "node",
    "express",
    "mysql",
    "knex"
  ],
  "author": "AAkrit Subedi",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.17",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  },
  "engines": {
    "node": "14.x"
  }
}
